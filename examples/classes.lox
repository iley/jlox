class Bagel {
  eat() {
    print "eating bagel";
  }
}

var bagel = Bagel();
bagel.foo = "foo";
bagel.bar = "bar";
print bagel.foo + bagel.bar;

fun f() {
  print "f";
}
bagel.f = f;
bagel.f();

bagel.eat();


class Counter {
  init() {
    print "counter init";
    this.val = 0;
  }

  inc() {
    print "counter inc";
    this.val = this.val + 1;
  }

  say() {
    print "counter say";
    print this.val;
  }
}

var c = Counter();
c.inc();
c.inc();
c.inc();
c.say();
print c.init();
